{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch and Attach to Server",
      "type": "coreclr",
      "request": "attach",
      "processName": "Outernet.Server.exe",
      "preLaunchTask": "Launch Server"
    },
    {
      "name": "Attach to Server",
      "type": "coreclr",
      "request": "attach",
      "processName": "Outernet.Server.exe"
    },
    {
      "name": "Attach to Editor",
      "type": "vstuc",
      "request": "attach",
      "projectPath": "${workspaceFolder}/Outernet.Client"
    },
    {
      "name": "Attach to Android",
      "type": "vstuc",
      "request": "attach",
      "endPoint": "127.0.0.1:56000",
      "preLaunchTask": "Find and Forward Debugger Port"
    }
  ]
}
