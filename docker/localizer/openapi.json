{
  "openapi": "3.1.0",
  "info": {
    "title": "Localize",
    "version": "0.1.0"
  },
  "paths": {
    "/health": {
      "get": {
        "summary": "Health Check",
        "operationId": "health_check",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "type": "object",
                  "title": "Response Health Check"
                }
              }
            }
          }
        }
      }
    },
    "/reconstructions/{id}": {
      "post": {
        "summary": "Load Reconstruction",
        "operationId": "load_reconstruction",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid",
              "title": "Id"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      },
      "delete": {
        "summary": "Unload Reconstruction",
        "operationId": "unload_reconstruction",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid",
              "title": "Id"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/reconstructions/{id}/status": {
      "get": {
        "summary": "Get Reconstruction Load Status",
        "operationId": "get_reconstruction_load_status",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid",
              "title": "Id"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoadStateResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/camera": {
      "put": {
        "summary": "Set Camera Intrinsics",
        "operationId": "set_camera_intrinsics",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "oneOf": [
                  {
                    "$ref": "#/components/schemas/PinholeCameraConfig"
                  },
                  {
                    "$ref": "#/components/schemas/OpenCVCameraConfig"
                  },
                  {
                    "$ref": "#/components/schemas/FullOpenCVCameraConfig"
                  }
                ],
                "title": "Camera",
                "discriminator": {
                  "propertyName": "model",
                  "mapping": {
                    "PINHOLE": "#/components/schemas/PinholeCameraConfig",
                    "OPENCV": "#/components/schemas/OpenCVCameraConfig",
                    "FULL_OPENCV": "#/components/schemas/FullOpenCVCameraConfig"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/localization": {
      "post": {
        "summary": "Localize Image",
        "operationId": "localize_image",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Body_localize_image"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/Localization"
                  },
                  "type": "array",
                  "title": "Response Localize Image"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Body_localize_image": {
        "properties": {
          "image": {
            "type": "string",
            "format": "binary",
            "title": "Image"
          }
        },
        "type": "object",
        "required": [
          "image"
        ],
        "title": "Body_localize_image"
      },
      "FullOpenCVCameraConfig": {
        "properties": {
          "width": {
            "type": "integer",
            "title": "Width"
          },
          "height": {
            "type": "integer",
            "title": "Height"
          },
          "mirroring": {
            "type": "string",
            "enum": [
              "None",
              "X",
              "Y"
            ],
            "title": "Mirroring"
          },
          "rotation": {
            "type": "string",
            "enum": [
              "None",
              "90_CW",
              "180",
              "90_CCW"
            ],
            "title": "Rotation"
          },
          "model": {
            "type": "string",
            "const": "FULL_OPENCV",
            "title": "Model"
          },
          "fx": {
            "type": "number",
            "title": "Fx"
          },
          "fy": {
            "type": "number",
            "title": "Fy"
          },
          "cx": {
            "type": "number",
            "title": "Cx"
          },
          "cy": {
            "type": "number",
            "title": "Cy"
          },
          "k1": {
            "type": "number",
            "title": "K1"
          },
          "k2": {
            "type": "number",
            "title": "K2"
          },
          "p1": {
            "type": "number",
            "title": "P1"
          },
          "p2": {
            "type": "number",
            "title": "P2"
          },
          "k3": {
            "type": "number",
            "title": "K3"
          },
          "k4": {
            "type": "number",
            "title": "K4"
          },
          "k5": {
            "type": "number",
            "title": "K5"
          },
          "k6": {
            "type": "number",
            "title": "K6"
          }
        },
        "type": "object",
        "required": [
          "width",
          "height",
          "mirroring",
          "rotation",
          "model",
          "fx",
          "fy",
          "cx",
          "cy",
          "k1",
          "k2",
          "p1",
          "p2",
          "k3",
          "k4",
          "k5",
          "k6"
        ],
        "title": "FullOpenCVCameraConfig"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "LoadState": {
        "type": "string",
        "enum": [
          "pending",
          "loading",
          "ready",
          "failed"
        ],
        "title": "LoadState"
      },
      "LoadStateResponse": {
        "properties": {
          "status": {
            "$ref": "#/components/schemas/LoadState"
          },
          "error": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Error"
          }
        },
        "type": "object",
        "required": [
          "status"
        ],
        "title": "LoadStateResponse"
      },
      "Localization": {
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "title": "Id"
          },
          "transform": {
            "$ref": "#/components/schemas/Transform"
          },
          "metrics": {
            "$ref": "#/components/schemas/LocalizationMetrics"
          }
        },
        "type": "object",
        "required": [
          "id",
          "transform",
          "metrics"
        ],
        "title": "Localization"
      },
      "LocalizationMetrics": {
        "properties": {
          "inlier_ratio": {
            "type": "number",
            "title": "Inlier Ratio"
          },
          "reprojection_error_median": {
            "type": "number",
            "title": "Reprojection Error Median"
          }
        },
        "type": "object",
        "required": [
          "inlier_ratio",
          "reprojection_error_median"
        ],
        "title": "LocalizationMetrics"
      },
